service vendor.usb /vendor/bin/hw/android.hardware.usb-service
    class hal
    user system
    group system shell wakelock
    capabilities WAKE_ALARM BLOCK_SUSPEND

on post-fs
    chown root system /sys/class/typec/port0/power_role
    chown root system /sys/class/typec/port0/data_role
    chown root system /sys/class/typec/port0/port_type
    chown root system /sys/class/typec/port0/port0.0/mode1/active
    chown root system /sys/devices/platform/53f1000.spmi/spmi-0/0-04/contaminant_detection
    chown root system /sys/devices/platform/53f1000.spmi/spmi-0/0-04/usb_limit_accessory_current
    chown root system /sys/devices/platform/53f1000.spmi/spmi-0/0-04/usb_limit_accessory_enable
    chown root system /sys/devices/platform/53f1000.spmi/spmi-0/0-04/usb_limit_sink_current
    chown root system /sys/devices/platform/53f1000.spmi/spmi-0/0-04/usb_limit_sink_enable
    chown root system /sys/devices/platform/53f1000.spmi/spmi-0/0-04/usb_limit_source_enable
    chown root system /sys/devices/platform/53f1000.spmi/spmi-0/0-04/update_sdp_enum_timeout
    chown root system /sys/devices/platform/goog_usb_role_sw/disable_usb_data
    chown root system /sys/devices/platform/google,pogo/enable_usb
    chown root system /sys/devices/platform/c600000.dwc_dptx/drm-displayport/hpd
    chown root system /sys/devices/platform/c600000.dwc_dptx/drm-displayport/irq_hpd
    chown root system /sys/devices/platform/c600000.dwc_dptx/drm-displayport/link_lanes
    chown root system /sys/devices/platform/c600000.dwc_dptx/drm-displayport/link_rate
    chown root system /sys/devices/platform/c600000.dwc_dptx/drm-displayport/link_status
    chown root system /sys/devices/platform/c600000.dwc_dptx/drm-displayport/orientation
    chown root system /sys/devices/platform/c600000.dwc_dptx/drm-displayport/pin_assignment
    chown root system /sys/devices/platform/c600000.dwc_dptx/drm-displayport/usbc_cable_disconnect
    chmod 664 /sys/class/typec/port0/power_role
    chmod 664 /sys/class/typec/port0/data_role
    chmod 664 /sys/class/typec/port0/port_type
    chmod 664 /sys/class/typec/port0/port0.0/mode1/active
    chmod 664 /sys/devices/platform/goog_usb_role_sw/disable_usb_data
    chmod 664 /sys/devices/platform/53f1000.spmi/spmi-0/0-04/contaminant_detection
    chmod 664 /sys/devices/platform/53f1000.spmi/spmi-0/0-04/usb_limit_accessory_current
    chmod 664 /sys/devices/platform/53f1000.spmi/spmi-0/0-04/usb_limit_accessory_enable
    chmod 664 /sys/devices/platform/53f1000.spmi/spmi-0/0-04/usb_limit_sink_current
    chmod 664 /sys/devices/platform/53f1000.spmi/spmi-0/0-04/usb_limit_sink_enable
    chmod 664 /sys/devices/platform/53f1000.spmi/spmi-0/0-04/usb_limit_source_enable
    chmod 664 /sys/devices/platform/53f1000.spmi/spmi-0/0-04/update_sdp_enum_timeout
    chmod 664 /sys/devices/platform/google,pogo/enable_usb
    chmod 664 /sys/devices/platform/c600000.dwc_dptx/drm-displayport/hpd
    chmod 664 /sys/devices/platform/c600000.dwc_dptx/drm-displayport/irq_hpd
    chmod 664 /sys/devices/platform/c600000.dwc_dptx/drm-displayport/link_lanes
    chmod 664 /sys/devices/platform/c600000.dwc_dptx/drm-displayport/link_rate
    chmod 664 /sys/devices/platform/c600000.dwc_dptx/drm-displayport/link_status
    chmod 664 /sys/devices/platform/c600000.dwc_dptx/drm-displayport/orientation
    chmod 664 /sys/devices/platform/c600000.dwc_dptx/drm-displayport/pin_assignment
    chmod 664 /sys/devices/platform/c600000.dwc_dptx/drm-displayport/usbc_cable_disconnect
